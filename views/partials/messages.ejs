<% if(typeof errors != 'undefined') { %>
    <% errors.forEach(function(error){ %>
        <div class="alert alert-warning" role="alert">
            <%= error.msg %> <br>
        </div>
    <%  }); %>
<% } %>

<% if (success_msg != ''){ %>
    <div class="alert alert-success" role="alert">
        <%= success_msg %> <br>
    </div>
<% } %>

<% if (error_msg != ''){ %>
    <div class="alert alert-danger" role="alert">
        <%= error_msg %> <br>
    </div>
<% } %>

<% if (error != ''){ %>
    <div class="alert alert-danger" role="alert">
        <%= error %> <br>
    </div>
<% } %>